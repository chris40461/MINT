# SKKU-INSIGHT Frontend 환경 변수

# API 서버 URL
# 개발: http://localhost:8000/api/v1
# 프로덕션: https://your-api-domain.com/api/v1
VITE_API_BASE_URL=http://localhost:8000/api/v1

# API 요청 타임아웃 (ms)
VITE_API_TIMEOUT=30000

# React Query 설정
VITE_QUERY_STALE_TIME=300000  # 5분 (ms)
VITE_QUERY_GC_TIME=1800000    # 30분 (ms)

# 개발 환경 설정
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug  # debug, info, warn, error

# 애플리케이션 정보
VITE_APP_NAME=SKKU-INSIGHT
VITE_APP_VERSION=0.1.0
